# (=)
x = 5

print (x)

# (+=)
x = 9
x +=3

print (x)

# (-=)

x = 9
x -=3

print (x)

# (*=)

x = 9
x *=3

print (x)

# (/=)

x = 9
x /=3

print (x)

# (%=)

x = 9
x %=3

print (x)

# (//=)

x = 9
x //=3

print (x)

# (**=)

x = 9
x //=3

print (x)

# (&=)

x = 9
x &=3

print (x)


# (>>=)

x = 9
x >>=3

print (x)




